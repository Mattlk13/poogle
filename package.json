{
  "name": "emwas-api",
  "version": "1.0.1",
  "description": "Porn search utility",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "npx tslint 'src/**/*.ts' 'test/**/*.ts' --format stylish",
    "lint:fix": "npx tslint 'src/**/*.ts' 'test/**/*.ts' --fix -q",
    "lint:watch": "onchange 'src/**/*.ts' 'test/**/*.ts' -- npm run lint:fix",
    "build": "npx rimraf build/* && npx tsc",
    "build:watch": "tsc -w",
    "dev": "npm run build && node build/index.js",
    "start": "export PORT=8081 && npm install && npm run build && node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabienleite/emwas.git"
  },
  "keywords": [
    "porn",
    "search"
  ],
  "author": "Fabien Leite",
  "license": "CECILL-2.1",
  "bugs": {
    "url": "https://github.com/fabienleite/emwas/issues"
  },
  "homepage": "https://github.com/fabienleite/emwas#readme",
  "dependencies": {
    "aws-serverless-express": "^3.3.6",
    "axios": "^0.18.0",
    "body-parser": "^1.18.0",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cheerio": "^0.22.10",
    "@types/express": "^4.1.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.9.6",
    "@types/supertest": "^2.0.7",
    "jest": "^24.7.1",
    "onchange": "^5.2.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.0"
  }
}
