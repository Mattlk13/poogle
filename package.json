{
  "name": "emwas-api",
  "version": "1.0.1",
  "description": "Porn search utility",
  "scripts": {
    "test-no-cov": "mocha",
    "test": "nyc mocha",
    "codecov": "nyc report --reporter=json && mv coverage/coverage-final.json coverage/coverage.json && codecov",
    "build": "gulp build",
    "lint": "gulp tslint",
    "dev": "gulp build && node build/index.js",
    "start": "export PORT=8081 && npm install && npx gulp build && node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabienleite/emwas.git"
  },
  "keywords": [
    "porn",
    "search"
  ],
  "author": "Fabien Leite",
  "license": "CECILL-2.0",
  "bugs": {
    "url": "https://github.com/fabienleite/emwas/issues"
  },
  "homepage": "https://github.com/fabienleite/emwas#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.0",
    "cheerio": "^1.0.0-rc.2",
    "debug": "^4.1.1",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/cheerio": "^0.22.10",
    "@types/debug": "^4.1.0",
    "@types/express": "^4.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.6",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codecov": "^3.2.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^5.0.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.0"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ]
  }
}
